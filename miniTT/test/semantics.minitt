id : Π a : U . a → a
   = λ a . λ x . x
;

bool : U 
     = Sum (True 1 | False 1)
;

true : bool
     = True 0
;

false : bool
      = False 0
;

-- failed one
elimBool : Π c : (bool → U) . c false → c true → Π b : bool . c b
         = λ c . λ h0 . λ h1 . fun (True _ → h1 | False _ → h0)
; 
-- the system cannot determine that unit type has only one inhibitance zero, it's weird
-- when type-checking `λ _ . h1`, the type-checker checks it against `Π x₃ : 1 . x₀ (True x₃)`, but with inferred type of `h₁ : x₀ (True 0)` 