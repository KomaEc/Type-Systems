
id : Π a : U . a → a
   = λ a . λ x . x
;

bool : U 
     = Sum (True 1 | False 1)
;

test : U
     = Π c : a → b . 1
;

test : U
     = Π c : a → b . e c → d c
;
-- the problem is, this dot is reduced with 'b' !
-- to resolve, set precedence of '.' to be lower than '→'

elimBool : Π c : (bool → U) . c (False 0) → c (True 0) → Π b : bool . c b 
         = λ c . λ h0 . λ h1 . fun (False _ → h0 | True _ → h1)
;